{
  "name": "todo",
  "version": "1.0.0",
  "description": "A simple To-Do App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Shopping List\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/pngIcon.png --prune=true --out=release-builds"
  },
  "author": "Saaheer Purav",
  "license": "ISC",
  "dependencies": {
    "electron": "^11.0.2",
    "electron-reload": "^1.5.0",
    "python-shell": "^2.0.3"
  }
}
